{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/express"
  },
  "defaultProject": "server-graphql-api",
  "projects": {
    "server-graphql-api": {
      "root": "apps/server/graphql-api",
      "sourceRoot": "apps/server/graphql-api/src",
      "projectType": "application",
      "prefix": "graphql-api",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/server/graphql-api",
            "main": "apps/server/graphql-api/src/main.ts",
            "tsConfig": "apps/server/graphql-api/tsconfig.app.json",
            "assets": ["apps/server/graphql-api/src/assets"],
            "webpackConfig": "apps/server/graphql-api/webpack.config.ts"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/server/graphql-api/src/environments/environment.ts",
                  "with": "apps/server/graphql-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "server-graphql-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/server/graphql-api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/server/graphql-api"],
          "options": {
            "jestConfig": "apps/server/graphql-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
